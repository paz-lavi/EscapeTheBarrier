<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor"
    tools:context=".HighScores">
    <TextView
        android:id="@+id/high_LBL_highscore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:textSize="42sp"
        android:text="HIGE SCORES"
        android:textColor="@color/textColor"/>

    <TextView
        android:id="@+id/high_LBL_place1"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_highscore"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place2"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place3"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place4"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place5"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place6"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place7"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place8"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place7"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place9"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place8"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/high_LBL_place10"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/high_LBL_place9"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textColor="@color/textColor"
        android:textSize="12sp" />


<Button
    android:id="@+id/high_BT_maps"
    android:layout_width="200dp"
    android:layout_height="70dp"
    android:backgroundTint="@color/textColor"
    android:text="MAPS"
    android:textSize="36sp"
    android:layout_alignParentBottom="true"
    android:layout_centerHorizontal="true"
    android:textColor="@color/backgroundColor"/>



</RelativeLayout>